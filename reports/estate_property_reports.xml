<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!-- Property Offer report action -->
    <record id="action_report_property" model="ir.actions.report">
        <field name="name">Property Details</field>
        <field name="model">real.estate</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">egp_property.report_property_offers</field>
        <field name="report_file">egp_property.report_property_offers</field>
        <field name="binding_type">report</field>
        <field name="binding_model_id" ref="egp_property.model_real_estate"/>
    </record>


    <!-- property details report action    -->
    <record id="action_report_property_details" model="ir.actions.report">
        <field name="name">Property</field>
        <field name="model">real.estate</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">egp_property.report_property_details</field>
        <field name="report_file">egp_property.report_property_details</field>
        <field name="binding_type">report</field>
        <field name="binding_model_id" ref="egp_property.model_real_estate"/>
    </record>


</odoo>